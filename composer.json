{
  "name": "vantoozz/proxy-scraper",
  "license": "MIT",
  "type": "library",
  "description": "Free proxies scraper",
  "authors": [
    {
      "name": "Ivan Nikitin",
      "email": "vantoozz@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "php-http/client-implementation": "~1",
    "php-http/message-factory": "^1.0.2",
    "php-http/httplug": "^1.1",
    "php-http/message": "^1.5",
    "symfony/dom-crawler": "^3.3",
    "symfony/css-selector": "^3.3"
  },
  "require-dev": {
    "php-http/guzzle6-adapter": "~1",
    "squizlabs/php_codesniffer": "~3",
    "phploc/phploc": "~4",
    "sebastian/phpcpd": "~3",
    "phpmd/phpmd": "~2",
    "block8/php-docblock-checker": "~1",
    "jakub-onderka/php-parallel-lint": "~0",
    "phpstan/phpstan": "~0",
    "phpunit/phpunit": "~6",
    "codacy/coverage": "~1",
    "satooshi/php-coveralls": "~1",
    "league/container": "~2",
    "povils/phpmnd": "~1"
  },
  "suggest": {
    "php-http/guzzle6-adapter": "to use Guzzle as HTTP client"
  },
  "autoload": {
    "psr-4": {
      "Vantoozz\\ProxyScraper\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vantoozz\\ProxyScraper\\UnitTests\\": [
        "tests/unit"
      ],
      "Vantoozz\\ProxyScraper\\IntegrationTests\\": [
        "tests/integration"
      ],
      "Vantoozz\\ProxyScraper\\SystemTests\\": [
        "tests/system"
      ]
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "scripts": {
    "post-install-cmd": [
      "bash .githooks/setup.sh"
    ],
    "post-update-cmd": [
      "bash .githooks/setup.sh"
    ]
  }
}
